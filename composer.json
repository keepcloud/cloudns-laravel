{
    "name": "keepcloud/cloudns",
    "description": "A Laravel package for ClouDNS API.",
    "keywords": [
        "keepcloud",
        "cloudns",
        "api",
        "laravel"
    ],
    "homepage": "https://keepcloud.io",
    "license": "MIT",
    "type": "laravel-package",
    "authors": [
        {
            "name": "Keepcloud",
            "email": "edison@keepcloud.io"
        }
    ],
    "require": {
        "php": "^7.4|^8.1",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Keepcloud\\ClouDNS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keepcloud\\ClouDNS\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Keepcloud\\ClouDNS\\ClouDNSServiceProvider"
            ],
            "aliases": {
                "ClouDNS": "Keepcloud\\ClouDNS\\ClouDNSFacade"
            }
        }
    }
}
